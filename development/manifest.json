{
  "manifest_version": 2,
  "name": "Tab Limiter",
  "version": "1.0",
  "description": "Limits the number of tabs that can be open in a session",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "notifications"
  ],
    "background": {
    "scripts": ["./src/main.js"]
  },
  "options_ui": {
    "page": "./src/options/index.html",
    "browser_style": true
  }
}