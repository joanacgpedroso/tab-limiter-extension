{
  "manifest_version": 2,
  "name": "Tab Limiter",
  "version": "1.0",
  "description": "Limits the number of tabs that can be open in a session",
  "permissions": [
    "<all_urls>",
    "tabs",
    "activeTab"
  ],
    "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "browser_style": true
  }
}